    "@types/alpinejs": "^3.13.11",
    "@types/node": "^22.10.1",
    "@vercel/og": "^0.6.4",
    "alpinejs": "^3.14.7",
    "astro": "^5.1.4",
    "chalk": "^5.3.0",
